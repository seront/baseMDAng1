{
  "name": "medipass-base",
  "version": "1.0.0",
  "description": "Componentes minimos necesarios para los multiples proyectos frontend de Medipass",
  "main": "lib/medipass-semilla",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch start-mockapi",
    "open:src": "babel-node buildScripts/srcServer.js",
    "open:src:nodemon": "nodemon buildScripts/srcServer.js --exec babel-node",
    "security-check": "nsp check",
    "lint": "esw webpack.config.* src buildScripts ",
    "lint:watch": "npm run lint -- --watch",
    "share": "npm-run-all --parallel localtunnel open:src ",
    "localtunnel": "lt --port 3000",
    "test": "mocha --reporter progress buildScripts/testSepup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js"
  },
  "author": "NÃ©stor Mendoza",
  "license": "BSD-2-Clause",
  "dependencies": {
    "angular": "^1.6.4",
    "angular-ui-router": "^1.0.3",
    "axios": "^0.16.2",
    "bootstrap": "^3.3.7",
    "bootswatch": "^3.3.7",
    "file-loader": "^0.11.2",
    "image-webpack-loader": "^3.3.1",
    "jquery": "^3.2.1",
    "json-loader": "^0.5.4",
    "promise": "^8.0.0",
    "url-loader": "^0.5.9",
    "webpack-isomorphic-tools": "^3.0.3",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "chalk": "^1.1.3",
    "cheerio": "^1.0.0-rc.1",
    "compression": "^1.6.2",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.0.0",
    "eslint-plugin-import": "^2.5.0",
    "eslint-watch": "^3.1.2",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^11.0.0",
    "json-schema-faker": "^0.5.0-rc6",
    "json-server": "^0.10.1",
    "localtunnel": "^1.8.3",
    "mocha": "^3.4.2",
    "nock": "^9.0.13",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.3",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "0.0.5"
  }
}
